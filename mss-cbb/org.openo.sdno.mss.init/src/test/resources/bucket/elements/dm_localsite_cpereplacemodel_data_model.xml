<?xml version="1.0" encoding="utf-8"?>
<inventory xmlns="http://www.w3school.com.cn" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.w3school.com.cn inv-datamodel-1.0.xsd">
    <datamodel name="inv_localsite_cpereplacemodel">
        <index name="idx_localsite_cpereplacemodel_uniqueid" value="uuid" isunique="true" />
        <index name="idx_localsite_cpereplacemodel_esn" value="newEsn" isunique="true" />
        <filter name="default" value="uuid in (:uuid) and newEsn in (:newEsn) and oldEsn in (:oldEsn)" />
    </datamodel>
</inventory>
