<?xml version="1.0" encoding="utf-8"?>
<inventory xmlns="http://www.w3school.com.cn" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3school.com.cn inv-infomodule-1.0.xsd">
    <infomodel name="controller" datamodel="inv_controller" version="1.0">
        <!--basic property-->
        <basic>
            <property name="name" type="string" length="255"/>
            <property name="objectId" type="string" length="36"/>
            <property name="category" type="string" length="36"/>
			<property name="productName" type="string" length="32"/>
			<property name="version" type="string" length="64"/>
			<property name="hostName" type="string" length="128"/>
			<property name="slaveHostName" type="string" length="128"/>
			<property name="description" type="string" length="255"/>
			<property name="vendor" type="string" length="64"/>
        </basic>

        <!-- extension  -->
        <extension>
        </extension>
      </infomodel>
</inventory>