<?xml version="1.0" encoding="utf-8"?>
<inventory xmlns="http://www.w3school.com.cn" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.w3school.com.cn inv-infomodule-1.0.xsd">
    <infomodel name="terminationpoint" datamodel="inv_terminationpoint" version="1.0">
        <!--basic property -->
        <basic>
            <property name="name" type="string" length="255"/>
            <property name="meID" type="string" length="36"/>
            <property name="logicalType" type="string" length="64"/>
            <property name="layerRate" type="string" length="32"/>
            <property name="isEdgePoint" type="string" length="32"/>
            <property name="portIndex" type="string" length="255"/>
            <property name="source" type="string" length="32"/>
            <property name="owner" type="string" length="36"/>
            <property name="ipAddress" type="string" length="128"/>
            <property name="adminState" type="string" length="32"/>
            <property name="nativeID" type="string" length="64"/>
        </basic>

        <!-- extension -->
        <extension>
            <property name="description" type="string" length="255"/>
            <property name="direction" type="string" length="64"/>
            <property name="operState" type="string" length="32"/>
            <property name="phyBW" type="string" length="64"/>
            <property name="ipMask" type="string" length="64"/>
        </extension>
    </infomodel>
</inventory>